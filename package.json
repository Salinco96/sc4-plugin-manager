{
  "name": "sc4-plugin-manager",
  "version": "0.1.0",
  "description": "SC4 Plugin Manager interface using Electron",
  "homepage": "https://github.com/Salinco96/sc4-plugin-manager#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Salinco96/sc4-plugin-manager.git"
  },
  "bugs": {
    "url": "https://github.com/Salinco96/sc4-plugin-manager/issues"
  },
  "packageManager": "yarn@4.3.0",
  "main": "./out/main/index.js",
  "author": "Salinco96",
  "license": "GPL-3.0",
  "scripts": {
    "build": "electron-vite build",
    "dev": "electron-vite dev --watch",
    "format": "prettier --ignore-path .gitignore --write .",
    "indexer": "ts-node --project tsconfig.scripts.json -r tsconfig-paths/register src/scripts/simtropolis/indexer.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix --ignore-path .gitignore --max-warnings 0",
    "make": "yarn build && yarn package",
    "package": "electron-builder --win",
    "postinstall": "husky install && electron-builder install-app-deps",
    "precommit": "yarn lint-staged",
    "preview": "electron-vite preview",
    "typecheck": "yarn typecheck:main && yarn typecheck:renderer && yarn typecheck:scripts",
    "typecheck:main": "tsc --noEmit -p tsconfig.main.json --composite false",
    "typecheck:renderer": "tsc --noEmit -p tsconfig.renderer.json --composite false",
    "typecheck:scripts": "tsc --noEmit -p tsconfig.scripts.json --composite false"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.15.17",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.17",
    "content-disposition": "^0.5.4",
    "electron-log": "^5.1.4",
    "escape-html": "^1.0.3",
    "glob": "^10.4.1",
    "i18next": "^23.11.5",
    "immutability-helper": "^3.1.1",
    "isomorphic-git": "^1.25.10",
    "notistack": "^3.0.1",
    "react-i18next": "^14.1.2",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.10",
    "unzipper": "^0.11.6",
    "yaml": "^2.4.2",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/content-disposition": "^0.5.8",
    "@types/escape-html": "^1",
    "@types/node": "^18.19.9",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-window": "^1.8.8",
    "@types/unzipper": "^0.10.9",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv": "^16.4.5",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.5",
    "node-html-parser": "^6.1.13",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
