{
  "name": "sc4-plugin-manager",
  "version": "1.0.0-alpha.2",
  "description": "SC4 Plugin Manager interface using Electron",
  "homepage": "https://github.com/Salinco96/sc4-plugin-manager#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Salinco96/sc4-plugin-manager.git"
  },
  "bugs": {
    "url": "https://github.com/Salinco96/sc4-plugin-manager/issues"
  },
  "engines": {
    "node": ">=20.0.0 <22.0.0"
  },
  "packageManager": "yarn@4.3.0",
  "main": "./out/main/index.js",
  "author": "Salinco96 <loicnico96b@gmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "build": "electron-vite build",
    "dev": "electron-vite dev --watch",
    "indexer": "ts-node --project tsconfig.scripts.json -r tsconfig-paths/register src/scripts/indexer.ts",
    "lint": "biome check --fix",
    "make": "yarn build && yarn package",
    "package": "electron-builder --win",
    "postinstall": "husky install && electron-builder install-app-deps",
    "precommit": "lint-staged",
    "preview": "electron-vite preview",
    "typecheck": "yarn typecheck:main && yarn typecheck:renderer && yarn typecheck:scripts",
    "typecheck:main": "tsc --noEmit -p tsconfig.main.json --composite false",
    "typecheck:renderer": "tsc --noEmit -p tsconfig.renderer.json --composite false",
    "typecheck:scripts": "tsc --noEmit -p tsconfig.scripts.json --composite false"
  },
  "dependencies": {
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@fontsource/roboto": "5.0.13",
    "@inquirer/prompts": "7.1.0",
    "@mui/icons-material": "5.15.18",
    "@mui/lab": "5.0.0-alpha.170",
    "@mui/material": "5.15.18",
    "@salinco/nice-utils": "1.0.0-alpha.9",
    "@uiw/react-color-sketch": "2.3.2",
    "apexcharts": "3.54.1",
    "content-disposition": "0.5.4",
    "electron-log": "5.1.4",
    "escape-html": "1.0.3",
    "glob": "10.4.1",
    "i18next": "23.11.5",
    "immutability-helper": "3.1.1",
    "isomorphic-git": "1.25.10",
    "js-yaml": "4.1.0",
    "notistack": "3.0.1",
    "qfs-compression": "0.2.1",
    "react-apexcharts": "1.5.0",
    "react-i18next": "14.1.2",
    "react-markdown": "9.0.1",
    "react-material-ui-carousel": "3.4.2",
    "react-virtuoso": "4.12.0",
    "remark-gfm": "4.0.0",
    "unzipper": "0.11.6",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@joplin/turndown": "4.0.73",
    "@joplin/turndown-plugin-gfm": "1.0.55",
    "@types/content-disposition": "0.5.8",
    "@types/escape-html": "1.0.4",
    "@types/js-yaml": "4.0.9",
    "@types/node": "20.17.6",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/react-transition-group": "4.4.11",
    "@types/turndown": "5.0.4",
    "@types/unzipper": "0.10.9",
    "@vitejs/plugin-react": "4.2.1",
    "dotenv": "16.4.5",
    "electron": "33.2.0",
    "electron-builder": "25.1.8",
    "electron-vite": "2.3.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.10",
    "node-html-parser": "6.1.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.5.4",
    "vite": "5.2.14"
  }
}
